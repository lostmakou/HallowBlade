<Application x:Class="ZeldaWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ZeldaWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="EpilepsySans">Data\Font\EpilepsySans.ttf#Epilepsy Sans</FontFamily>
        <FontFamily x:Key="EpilepsySansBold">Data\Font\EpilepsySansBold.ttf#Epilepsy Sans</FontFamily>
        <BitmapImage x:Key="AppIcon" UriSource="/Data/Texture/Pumpkin.png"/>

    </Application.Resources>
</Application>
